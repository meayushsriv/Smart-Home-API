	<section id="features">
		<div class="container">
			<div class="row">
				<div class="col-xs-12">
					<div class="section-header">
						Smart Home
					</div>
				</div><!-- /.col -->
			</div><!-- /.row -->

			<div class="row">
				<div class="unique-container">
					<div class="unique-card">
						<div class="unique-box">
							<div class="unique-content">
								<h2></h2>
								<h3>Fan</h3>
								<img src="/public/images/fan.png" height="50px" width="50px"><br>
								<form action="/submitfan" method="post">
									<div class="speed-dropdown">

										<label for="speed">Speed:</label>
										<select id="speed" name="value">
											<option value="0">0</option>
											<option value="1">1</option>
											<option value="2">2</option>
											<option value="3">3</option>
											<option value="4">4</option>
											<option value="5">5</option>
										</select>
									</div>
									<button type="submit">Submit</button>
								</form>
								<div class="current-color-input">
									<label for="current-color" class="new">Current State:</label>
									<div class="current-color" id="data">
										<%= data.fan %>
									</div>
								</div>
							</div>
						</div>
					</div>

					<div class="unique-card">
						<div class="unique-box">
							<div class="unique-content">
								<h2></h2>
								<h3>Bulb</h3>
								<img src="/public/images/bulb.png" height="50px" width="50px"><br>
								<form action="/submitbulb" method="post">
									<div class="speed-dropdown">
										<label for="speed">State:</label>
										<select id="bstate" name="value">
											<option value="1">On</option>
											<option value="0">Off</option>
										</select>
									</div>

									<button type="submit">Submit</button>
								</form>
								<div class="current-color-input">
									<label for="current-color" class="new">Current State:</label>
									<div class="current-color" id="data">
										<%= data.bulb %>hghg
									</div>
								</div>

							</div>
						</div>
					</div>

					<div class="unique-card">
						<div class="unique-box">
							<div class="unique-content">
								<h2></h2>
								<h3>AC</h3>
								<img src="/public/images/ac.png" height="50px" width="50px"><br>
								<form action="/submitac" method="post">
									<div class="speed-dropdown">
										<label for="state">State:</label>
										<select id="speed" name="value">
											<option value="0">On</option>
											<option value="1">Off</option>
										</select>
										<div class="speed-dropdown">
											<label for="speed">Temperature:</label>
											<select id="speed" name="speed">
												<option value="16">16</option>
												<option value="17">17</option>
												<option value="18">18</option>
												<option value="19">19</option>
												<option value="20">20</option>
												<option value="21">21</option>
												<option value="22">22</option>
												<option value="23">23</option>
												<option value="23">24</option>
												<option value="23">25</option>
												<option value="23">26</option>
												<option value="23">27</option>
												<option value="23">28</option>
												<option value="23">29</option>
												<option value="23">30</option>
											</select>
										</div>

									</div>
									<button type="submit">Submit</button>
								</form>
								<div class="current-color-input">
									<label for="current-color" class="new">Current Temperature:</label>
									<div class="current-color" id="data">
										<%= data.ac.temp %>
											<%= data.ac.state %>

									</div>
									<!-- <input type="text" id="current-color" name="current-color" readonly> -->
								</div>

							</div>
						</div>
					</div>
					<div class="unique-card">
						<div class="unique-box">
							<div class="unique-content">
								<h2></h2>
								<h3>LED</h3>
								<img src="/public/images/led.png" height="50px" width="50px"><br>
								<form action="/submitled" method="post">
									<div class="speed-dropdown">
										<label for="speed">Color:</label>
										<select id="speed" name="value">
											<option value="#000000">#000000</option>
											<option value="#51FF00">#51FF00</option>
											<option value="#FF8700">#FF8700</option>
											<option value="#FF0000">#FF0000 </option>
											<option value="#0042FF">#0042FF</option>
											<option value="#00F3FF">#00F3FF</option>
										</select>
									</div>

									<button type="submit">Submit</button>
								</form>
								<div class="current-color-input">
									<label for="current-color" class="new">Current Color:</label>
									<div class="current-color" id="data">
										<%= data.led %>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>


			</div><!-- /.row -->
		</div><!-- /.container --><br><br><br><br><br><br><br>

	</section><!-- /#features -->

